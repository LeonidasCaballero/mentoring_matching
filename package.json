  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "openai": "^4.28.0",
    "p-limit": "^3.1.0",
    "react-scripts": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
