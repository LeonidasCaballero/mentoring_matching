{
  "name": "mentoring-matcher",
  "version": "1.0.0",
  "description": "Sistema de matching entre mentores y mentees",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd client && npm start\"",
